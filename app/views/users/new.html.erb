<%= form_for @user do |f| %>
  <p>
    <%= f.label :fname, "First Name" %>
    <%= f.text_field :fname %>
  </p>
  <p>
    <%= f.label :lname, "Last Name" %>
    <%= f.text_field :lname %>
  </p>
  <p>

    <%= f.label :login %>
    <%= f.text_field :login %>
  </p>
  <p>

    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>
  <p>

    <%= f.label :password_confirmation, "Confirm Password" %>
    <%= f.text_field :password_confirmation %>
  </p>
  <p>
    <%= f.submit "Register" %>
  </p>
<% end %>